<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="150.0" prefWidth="200.0" xmlns="http://javafx.com/javafx/11.0.1"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.synoptic.controller">
    <Label layoutX="14.0" layoutY="14.0" prefHeight="52.0" prefWidth="296.0" text="Information Fact File"/>
    <VBox layoutX="15.0" layoutY="66.0" prefHeight="647.0" prefWidth="371.0">
        <Label prefHeight="25.0" prefWidth="194.0" text="Lobitos &amp; Piedritas" wrapText="true"/>
        <Pane prefHeight="300.0" prefWidth="371.0">
            <GridPane layoutX="-1.0" layoutY="-1.0" prefHeight="611.0" prefWidth="370.0">
                <columnConstraints>
                    <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" minWidth="10.0"
                                       prefWidth="100.0"/>
                    <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" minWidth="10.0"
                                       prefWidth="100.0"/>
                </columnConstraints>
                <rowConstraints>
                    <RowConstraints minHeight="10.0" prefHeight="30.0" valignment="TOP"
                                    vgrow="SOMETIMES"/>
                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                </rowConstraints>
                <Button fx:id="localAreaButton" alignment="CENTER" contentDisplay="CENTER"
                        mnemonicParsing="false" onAction="#localAreaButton" prefHeight="72.0"
                        prefWidth="145.0" text="The Local Area" textAlignment="CENTER"
                        GridPane.rowIndex="2"/>
                <Button fx:id="demographicButton" alignment="CENTER" contentDisplay="CENTER"
                        mnemonicParsing="false" onAction="#demographicButton" prefHeight="72.0"
                        prefWidth="145.0" text="Demographics" textAlignment="CENTER"
                        GridPane.columnIndex="1" GridPane.rowIndex="1"/>
                <Button fx:id="industriesAndEmploymentButton" alignment="CENTER"
                        contentDisplay="CENTER" mnemonicParsing="false"
                        onAction="#industriesAndEmploymentButton" prefHeight="72.0"
                        prefWidth="145.0" text="Industries &amp; Employment"
                        textAlignment="CENTER" wrapText="true" GridPane.columnIndex="1"
                        GridPane.rowIndex="2"/>
                <Label prefHeight="93.0" prefWidth="117.0" text="Lobitos Population: 1,300"
                       textAlignment="CENTER" wrapText="true" GridPane.valignment="CENTER"/>
                <Button fx:id="climateButton" alignment="CENTER" contentDisplay="CENTER"
                        layoutX="215.0" layoutY="76.0" mnemonicParsing="false"
                        onAction="#climateButton" prefHeight="72.0" prefWidth="145.0"
                        text="Climate" textAlignment="CENTER" GridPane.rowIndex="1"/>
                <Label prefHeight="93.0" prefWidth="117.0" text="Piedritas Population: 400"
                       textAlignment="CENTER" wrapText="true" GridPane.columnIndex="1"
                       GridPane.valignment="CENTER"/>
            </GridPane>
        </Pane>
    </VBox>
</AnchorPane>
